{
  "_version": "1.12.0",
  "sap.app": {
    "id": "com.myorg.myapp",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "${version}"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {},
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "com.myorg.myapp.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.130.2",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.f": {},
        "sap.uxap": {}
      }
    },
    "handleValidation": true,
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.myorg.myapp.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/index.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "Type": "View",
        "viewType": "XML",
        "viewPath": "com.myorg.myapp.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
					"target": "notFound"
				},
        "async": true
      },
      "routes": [
        {
          "pattern": "employees/{employeeId}/resume:?query:",
					"name": "employeeResume",
					"target": "employeeResume"
				},
        {
         "pattern": "employeeDetail/{EmployeeID}",
         "name":"employeeDetail",
         "target":"employeeDetail"
        },
        {
          "pattern": "employee",
          "name": "employee",
          "target": "employee"
        },
        {
          "pattern": "home",
          "name": "home",
          "target": "home"
        },
        {
          "pattern": "",
          "name": "main",
          "target": "main"
        },
        {
          "pattern": "product/{productId}",
          "name": "productdescription",
          "target": [
            "Flexible,productDetail"
          ]
        },
        {
          "pattern": "flexible",
          "name": "flexible",
          "target": "flexible"
        },
        {
          "pattern": "detail/{layout}",
          "name": "detail",
          "target": [
            "flexible",
            "detail"
          ]
        }
      ],
      "targets": {
        "main": {
          "viewId": "main",
          "viewName": "Main"
        },
        "productDetail": {
          "viewName": "ProductDetail",
          "viewId": "productDetailView"
        },
        "flexible": {
          "viewName": "FlexibleColumn",
          "viewId": "flexiblecol"
        },
        "flexibleList": {
          "viewName": "flexibleList",
          "viewId": "flexiblelist"
        },
        "home": {
          "viewName": "Home",
          "viewId": "home"
        },
        "employee": {
         "id": "employeeList",
					"path": "com.myorg.myapp.view.employee",
					"viewName": "employee.Employee",
					"level": 2
        },
        "employeeDetail":{
         "id": "employeeDetail",
					"path": "com.myorg.myapp.view.employee",
					"viewName": "employee.EmployeeDetail",
					"level": 2
        },
        "employeeResume": {
					"viewType": "XML",
					"viewName": "employee.ResumeDetail",
					"viewId": "resumeDetail"
				},
        	"notFound": {
					"id": "notFound",
					"viewName": "NotFound",
					"transition": "show"
				}
      }
    }
  }
}
